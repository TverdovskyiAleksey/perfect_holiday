{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "@Components/*": [
        "Components/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@language/*": [
        "language/*"
      ],
    },

    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
